<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- is.type.article --> 
    <record id="is_type_article_form" model="ir.ui.view">
        <field name="name">is.type.article.form</field>
        <field name="model">is.type.article</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name" />
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <record id="is_type_article_tree" model="ir.ui.view">
        <field name="name">is.type.article.tree</field>
        <field name="model">is.type.article</field>
        <field name="arch" type="xml">
            <list>
                <field name="name" />
            </list>
        </field>
    </record>
    <record id="is_type_article_search" model="ir.ui.view">
        <field name="name">is.type.article.search</field>
        <field name="model">is.type.article</field>
        <field name="arch" type="xml">
            <search>
                <field name="name" />
           </search>
        </field>
    </record>
    <record  id="is_type_article_action" model="ir.actions.act_window">
        <field name="name">Type d'article</field>
        <field name="res_model">is.type.article</field>
        <field name="view_mode">list,form</field>
    </record>



