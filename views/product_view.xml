    <!-- product.template -->
    <record id="is_product_template_form_view" model="ir.ui.view">
        <field name="name">product_template_form_view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_form_view"/>
        <field name="arch" type="xml">
            <field name="categ_id" position="after">
                <field name="is_lien_documentation"  widget="url" placeholder="documentation"/>
                <field name="is_stock_mini"/>
                <field name="is_lot_reappro"/>
                <field name="is_type_article_id"/>
                <field name="is_famille_id"/>
                <field name="is_sous_famille1_id"/>
                <field name="is_sous_famille2_id"/>
                <field name="is_emplacement_stock_id"/>
                <field name="is_note"/>
            </field>
        </field>
    </record>
    <record id="is_product_template_tree_view" model="ir.ui.view">
        <field name="name">is_product_template_tree_view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_tree_view"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="is_type_article_id"/>
                <field name="is_famille_id"/>
                <field name="is_sous_famille1_id"/>
                <field name="is_sous_famille2_id"/>
            </field>
        </field>
    </record>
    <record id="is_view_stock_product_template_tree" model="ir.ui.view">
        <field name="name">is_view_stock_product_template_tree</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock.view_stock_product_template_tree"/>
        <field name="arch" type="xml">
            <field name="qty_available" position="before">
                <field name="is_emplacement_stock_id"/>
            </field>
            <field name="qty_available" position="after">
                <field name="is_stock_mini"/>
                <field name="is_qt_cde"/>
                <field name="is_lot_reappro"/>
            </field>
        </field>
    </record>
    <record id="is_product_template_search_view" model="ir.ui.view">
        <field name="name">is_product_template_search_view</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="product.product_template_search_view"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="is_type_article_id"/>
                <field name="is_famille_id"/>
                <field name="is_sous_famille1_id"/>
                <field name="is_sous_famille2_id"/>
                <field name="is_emplacement_stock_id"/>
                <field name="is_note"/>
            </field>
        </field>
    </record>

    <!-- product.supplierinfo -->
    <record id="is_product_supplierinfo_tree_view" model="ir.ui.view">
        <field name="name">is_product_supplierinfo_tree_view</field>
        <field name="model">product.supplierinfo</field>
        <field name="inherit_id" ref="product.product_supplierinfo_tree_view"/>
        <field name="arch" type="xml">
            <field name="partner_id" position="after">
                <field name="product_code"      optional="show"/>
                <field name="is_fabriquant"     optional="show"/>
                <field name="is_ref_fabriquant" optional="show"/>
            </field>
        </field>
    </record>
    <record id="is_product_supplierinfo_form_view" model="ir.ui.view">
        <field name="name">is_product_supplierinfo_form_view</field>
        <field name="model">product.supplierinfo</field>
